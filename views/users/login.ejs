<% layout("/layouts/boilerplate") %>

<div class=" row container mt-5 d-flex justify-content-center">
    <h1 class="col-md-8">Login</h1>
    <div class="col-md-8">
        <form action="/login" method="POST" class="needs-validation" novalidate >
            <div class="mb-3">
            <label for="username" class="form-label">Username</label>
            <input name="username" id="username" class="form-control"  type="text" required>
            <div class="valid-feedback">
                Looks good!
              </div>
            </div>
            
              <div class="mb-3">
                <label for="password" class="form-label">Password</label>
            <input name="password" id="password" class="form-control"  type="password" required>
            
               
            </div>
            <button class="btn btn-warning w-20 edit-btn">Login</button>
        </form>
    </div>
</div>
    
            
        

    
       
            
        
    